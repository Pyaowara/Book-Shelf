{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      }
    ],
    "env": {
      "DB_HOST": "@db_host",
      "DB_USER": "@db_user",
      "DB_PASS": "@db_pass",
      "DB_DATABASE": "@db_database",
      "JWT_SECRET": "@jwt_secret"
    }
  }
  