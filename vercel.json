{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/book-api",
            "dest": "/api/book-api.js"
        },
        {
            "src": "/api/user-api",
            "dest": "/api/user-api.js"
        }
    ]
}
